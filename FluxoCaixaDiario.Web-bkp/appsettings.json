{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Debug",
      "Microsoft.AspNetCore.Authentication.OpenIdConnect": "Trace",
      "Microsoft.Net.Http.Headers": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ServiceUrls": {
    "Lancamentos": "https://localhost:53036", // URL do Service de Lancamentos ou http://lancamentosservice se estiver no Docker
    "SaldoDiario": "https://localhost:4445" // URL do Service de SaldoDiario ou http://saldodiarioservice se estiver no Docker
  },
  "IdentityServer": {
    "Authority": "https://localhost:4435",
    "ClientId": "fluxocaixadiario.web",
    //"ClientSecret": "",
    "Scope": "openid profile fluxocaixadiario.lancamentos.api fluxocaixadiario.saldodiario.api fluxocaixadiario.web",
    "RedirectUri": "https://localhost:4430/signin-oidc",
    "PostLogoutRedirectUri": "https://localhost:4430/signout-callback-oidc"
  }
}